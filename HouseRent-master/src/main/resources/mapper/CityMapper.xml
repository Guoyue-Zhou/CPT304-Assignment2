<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.house.wym.dao.CityMapper">

<select id="selByName" resultType="java.lang.String">
    select city_code  from house.city WHERE
    house.city.city_name=#{cityName}
</select>
</mapper>